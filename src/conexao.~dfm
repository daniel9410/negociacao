  object qryMovimento: TADOQuery
    Connection = con
    Parameters = <>
    SQL.Strings = (
      'select m.*, p.nome_produtor, d.nome_distribuidor'
      '  from movimento m, '
      '       produtor p,'
      '       distribuidor d '
      ' where 1 = 1 '
      '  and p.cgc_produtor = m.cgc_produtor'
      'and d.cgc_distribuidor = m.cgc_distribuidor')
    Left = 360
    Top = 32
